<h3>Nous contacter</h3>
<%= form_with(model: @contact, local: true) do |f| %>
  <%= f.label :name, "Nom" %>
  <%= f.text_field :name %>
  <%= f.label :email, "adresse mail" %>
  <%= f.email_field :email, autocomplete: "email" %>
  <%= f.label :content, "Pas comme" %>
  <%= f.text_field :content %>
  <%= f.submit "Viens" %>
<% end %>